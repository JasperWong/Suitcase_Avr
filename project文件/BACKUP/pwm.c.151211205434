#include"pwm.h"


void io_init(void)
{
  DDRE=0xFF;
  PORTE|=(1<<1);
  DDRE&=~(1<<0);
  DDRA=0xff;
  DDRD=0xff;
  DDRD&=~(1<<2);
  PORTD|=(1<<2);
}

/************************************
函数名 :     t3_init
功能 :       t3定时初始化 ,8位快速PWM模式 ,无分频，OCR1A/B范围0-255
输入参数 :   无
返回参数 :   无
*************************************/
void t3_init(void)
{
   TCCR3A |=0xa9;//10101001
   TCCR3B |=0x09;//00001001
   OCR3A=0;
   OCR3B=0;
}